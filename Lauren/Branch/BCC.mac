;branch if carry is clear. 
;Opcode: 103000

START: 
	mov #7, R1 ;R1 = 000007
	ADC R1, #1 ;R1 = 000010. Carried
    BCC L1	;Shouldn't work

	ADC R1, #1 ;R1 = 000011. Didnt carry
    BCC L2	;Should work
    
	BR L3 	;Fallthru, Should never run!. Opcode:000400
    HALT

L1:
	mov #1, R1
    HALT
L2:
	mov #2, R1
    HALT

L3:
    mov #3, R1
    HALT

.END START
